datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model Producer {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  document       String   @unique
  name           String
  farmName       String
  city           String
  state          String
  totalArea      Int
  cultivableArea Int
  vegetationArea Int
  crops          Json
  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt
}
